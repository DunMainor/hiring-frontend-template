{
  "scripts": {
    "test": "jest",
    "lint": "npx eslint . --fix",
    "start": "npx react-native start",
    "reactotron:reverse": "adb reverse tcp:9090 tcp:9090",
    "build:debug": "npx react-native run-android --variant=debug",
    "clean:android": "yarn clear:caches && cd android && ./gradlew clean",
    "prepare:android": "yarn clean:android && yarn reactotron:reverse && yarn start",
    "clear:caches": "jest --clearCache && watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-*",
    "build:release:aab": "yarn clean:android && yarn bundle:release && yarn postinstall && cd android/ && ./gradlew bundleRelease",
    "build:release:apk": "yarn clean:android && yarn bundle:release && yarn postinstall && cd android/ && ./gradlew assembleRelease",
    "bundle:release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
